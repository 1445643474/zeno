QLabel[cssClass="proppanel"]
{
	font: 16px "Segoe UI Semibold";
	color: rgba(255, 255, 255, 0.7);
}

QLabel[cssClass="proppanel-nodename"]
{
	font: bold 15px "HarmonyOS Sans";
	color: rgba(255, 255, 255, 0.5);
}

QLabel[cssClass="proppanel-sectionname"]
{
	font: bold 12px "HarmonyOS Sans";
	color: rgba(187, 206, 220, 1.0);
}

QLabel[cssClass="proppanel-itemname"]
{
	font: bold 14px "HarmonyOS Sans";
	color: rgba(255, 255, 255, 0.7);
}

QLabel[cssClass="numslider"]
{
	font: 14px "HarmonyOS Sans";
	padding: 15px;
	text-align: center;
}

QLabel[cssClass = "welcomepage_name"]
{ 
	font: 36px "HarmonyOS Sans";
	color: rgb(255, 255, 255);
}

QLabel[cssClass = "welcomepage_label"]
{ 
	font: 14px "HarmonyOS Sans";
	color: rgb(255, 255, 255);
}

QLabel[cssClass = "welcomepage"]
{
	font: 14px "HarmonyOS Sans";
	color: rgb(133, 130, 128);
}

ZCurveMapEditor QLabel
{
	font: 14px "HarmonyOS Sans";
	color: rgb(133, 130, 128);
}

QLabel[cssClass="rgb"]
{
	padding: 0px;
	font: 12px "HarmonyOS Sans";
	color: #9D9DA3;
}

QLabel[cssClass="color_label"]
{
	font: 12px "HarmonyOS Sans";
	color: #8A8A8C;
}

QTableView[cssClass="proppanel"]
{
    border:0px solid white;
    color: rgb(166, 166, 166);
    gridline-color: rgb(23, 23, 23);
    background-color: rgb(28, 28, 28);
    alternate-background-color: rgb(20, 20, 20);
}

QHeaderView
{
    background-color: transparent;
}

QHeaderView::section
{
    color: rgb(166, 166, 166);
    background-color: rgb(39, 39, 39);
    border:1px solid rgb(23, 23, 23);
}

QTableView QTableCornerButton::section
{
    background-color: rgb(39, 39, 39);
    border:1px solid rgb(23, 23, 23);
}

QListView[cssClass="proppanel"]
{
    border:0px solid white;
    color: rgb(166, 166, 166);
    gridline-color: rgb(23, 23, 23);
    background-color: rgb(28, 28, 28);
    alternate-background-color: rgb(20, 20, 20);
}