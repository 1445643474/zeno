[+]
build_type=Debug
build_target=zenoedit
run_environ=ZENO_LOGLEVEL=debug ZENO_OPEN=/tmp/a.zsg
run_target="$(VIM:build_dir)/bin/zenoedit"
build_configs=-Wno-dev -DZENO_NO_WARNING:BOOL=ON -DZENO_BUILD_SHARED:BOOL=ON -DZENO_MULTIPROCESS:BOOL=ON -DZENO_INSTALL_TARGET:BOOL=ON -DZENO_WITH_ZenoFX:BOOL=ON -DZENO_WITH_zenvdb:BOOL=ON -DZENO_WITH_FEM:BOOL=ON -DZENO_WITH_Rigid:BOOL=ON -DZENO_WITH_TreeSketch:BOOL=ON -DZENO_WITH_Skinning:BOOL=ON -DZENO_WITH_cgmesh:BOOL=ON
